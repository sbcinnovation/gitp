{
  "name": "gitp",
  "version": "1.0.0",
  "description": "Pretty git branch exploration for the terminal",
  "main": "src/index.tsx",
  "type": "module",
  "scripts": {
    "start": "bun run src/index.tsx browse",
    "dev": "bun --watch src/index.tsx browse",
    "build": "bun build src/index.tsx --outdir dist --target node"
  },
  "dependencies": {
    "commander": "^14.0.0",
    "ink": "^5.2.1",
    "react": "^18.3.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@types/react": "^18.3.1",
    "react-devtools-core": "^4.28.0"
  },
  "keywords": [
    "git",
    "terminal",
    "branch",
    "explorer",
    "ink"
  ],
  "author": "",
  "license": "MIT"
}
